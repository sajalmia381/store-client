    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "11.1.2",
    "postcss": "^8.4.16",
    "prettier": "2.3.2",
    "tailwindcss": "^3.1.8",
