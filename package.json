{
  "name": "store-client",
  "version": "1.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "build": "ng build --configuration production --source-map=false",
    "deploy": "sudo scp -i ~/Desktop/pem/storeApi.pem -r ./dist/store-client/* ubuntu@ec2-3-7-68-106.ap-south-1.compute.amazonaws.com:/usr/share/nginx/html",
    "prettier": "prettier --write \"**/*.{js,json,css,scss,less,md,ts,html,component.html}\"",
    "test": "ng test",
    "dev:ssr": "ng run store-client:serve-ssr",
    "serve:ssr": "node dist/store-client/server/main.js",
    "build:ssr": "ng build && ng run store-client:server",
    "prerender": "ng run store-client:prerender"
  },
  "private": true,
  "dependencies": {
