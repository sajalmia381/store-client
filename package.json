    "@angular/platform-server": "^17.0.8",
    "@angular/router": "^17.0.8",
    "@angular/ssr": "^17.0.8",
    "@ngneat/tailwind": "^7.0.3",
    "@ngrx/effects": "^17.0.1",
    "@ngrx/entity": "^17.0.1",
